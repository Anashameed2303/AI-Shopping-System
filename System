user_history = ["electronics", "gadgets"]
recommendations = []

for category in user_history:
    if category == "electronics":
        recommendations.append("Smartphone")
    elif category == "gadgets":
        recommendations.append("Smartwatch")
    elif category == "gaming":
        recommendations.append("Gaming Consoles")
    else:
        recommendations.append("General Accessories")

print("Recommendations:", recommendations)

